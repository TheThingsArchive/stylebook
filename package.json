    "del": "^2.2.2",
    "diff": "^3.0.0",
    "fs-extra": "^0.30.0",
    "fs-readdir-recursive": "^1.0.0",
    "grunt": "^1.0.1",
    "grunt-mocha-test": "^0.12.7",
    "less2stylus": "^0.1.0",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
